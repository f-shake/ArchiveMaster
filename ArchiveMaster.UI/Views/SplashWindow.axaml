<Window
    x:Class="ArchiveMaster.Views.SplashWindow"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c="using:Avalonia.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:v="using:FzLib.Avalonia.Controls"
    xmlns:views="clr-namespace:ArchiveMaster.Views"
    xmlns:vm="using:ArchiveMaster.ViewModels"
    Title="文件归档大师"
    Width="256"
    Height="256"
    Background="{x:Null}"
    CanResize="False"
    ShowInTaskbar="False"
    SystemDecorations="None"
    Topmost="True"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <Image Source="/Assets/icon_light.png" />
</Window>
